{
  "name": "diversity-tracker",
  "version": "1.0.0",
  "description": "to start: cd client, run npm run build cd server, run npm run build cd server, run npm start",
  "main": "index.js",
  "engines": {
    "node": "8.11.3"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "preinstall": "cd client && npm install && npm run build",
    "postinstall": "cd server && npm install && npm run build",
    "start": "cd server && npm run start"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,css,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://eadehemingway@github.com/eadehemingway/diversity-tracker.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/eadehemingway/diversity-tracker/issues"
  },
  "homepage": "https://github.com/eadehemingway/diversity-tracker#readme",
  "devDependencies": {
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0"
  }
}
