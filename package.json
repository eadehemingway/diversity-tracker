{
  "name": "diversity-tracker",
  "version": "1.0.0",
  "description": "to start: cd client, run npm run build cd server, run npm run build cd server, run npm start",
  "main": "index.js",
  "engines": {
    "node": "8.11.3"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1", 
    "build:server": "cd server && npm run build",
    "build:client": "cd client && npm run build",
    "preinstall": "cd client && npm install && cd ../server && npm install",
    "postinstall": "npm run build:client && npm run build:server", 
    "start": "cd server && npm run start"

  },
